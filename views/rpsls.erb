<body>

<h1 id="title">Rock Paper Scissors Lizard Spock</h1>
<div id="welcomeMessage"></div>
    <form id="nameForm">
      <input id="nameInput" value="Click and type in your name here!" onclick="if (this.defaultValue==this.value) this.value=''" type="text" autocomplete="off">
    </form>
    <section id="images"> 
      <div class="image-container"> 
       <img data-pick="rock" src="/rock.png">
       <img data-pick="paper" src="/paper.png">
       <img data-pick="scissors" src="/scissors.png">
       <img data-pick="lizard" src="/lizard.png">
       <img data-pick="spock" src="/spock.png">
      </div>
    </section>
    <ul id="victoryMessage">
      
    </ul>
  

</body>
